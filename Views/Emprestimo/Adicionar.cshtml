@{
    ViewData["Title"] = "Adicionar emprestimo";
}

<div class="text-center">
    <h1 class="display-4">Adicionar emprestimo</h1>
</div>

<form class="mt-4 p-3">
  <div class="mb-3">
    <label for="exampleInputTitleBook" class="form-label">Titulo do livro</label>
    <input type="text" class="form-control" id="exampleInputTitleBook" aria-describedby="emailHelp">
  </div>

  <div class="mb-3">
    <label for="exampleInputAutor" class="form-label">Autor</label>
    <input type="text" class="form-control" id="exampleInputAutor">
  </div>

  <div class="mb-3">
    <label for="exampleInputQEFOE" class="form-label">Quem esta fazendo o emprestimo</label>
    <input type="number" class="form-control" id="exampleInputQEFOE">
  </div>

  <div class="mb-3 form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1">
    <label class="form-check-label" for="exampleCheck1">Check me out</label>
  </div>
  <button type="submit" class="btn btn-success">Confirmar</button>
  <a class="btn btn-secondary" asp-action="Index" asp-controller="Emprestimo">Voltar</a>
</form>